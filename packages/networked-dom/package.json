{
  "name": "@mml-io/networked-dom-server",
  "version": "0.0.39",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "/build",
    "/src"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "node ./build.js --build",
    "iterate": "node ./build.js --watch",
    "private-publish": "npm run build && publish-if-not-exists --access=restricted",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "@mml-io/networked-dom-document": "^0.0.39",
    "@mml-io/observable-dom": "^0.0.39",
    "@mml-io/observable-dom-common": "^0.0.39"
  }
}
